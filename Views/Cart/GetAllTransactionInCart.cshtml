@model EcommerceMVC.Models.DTO.Transation.TransationsResponseModel
<h1>All customer Transaction</h1>
<table class="table">
    <thead>
         <th>
                ReferenceNo
            </th>
            <th>
                ProductName
            </th>

            <th>
                Quantity
            </th>

            <th>
               TotalAmount 
            </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.Data)
        {
            <tr>
                 <td scope="row">
                    @item.ReferenceNo
                </td>

                <td scope="row">
                    @item.ProductName
                </td>
                <td scope="row">
                    @item.Quantity
                </td>
                <td scope="row">
                   @item.TotalAmount 
                </td>
                <td scope="row">
                </td>
                 <td scope="row">
                <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-primary">Edit</a>
                </td>
                 @* <td scope="row">
                   <a asp-action="GetAllTransactionInCart" asp-route-id="@item.Id" class="btn btn-primary">View</a>
                </td> *@
            </tr>
        }
    </tbody>
</table>
<form asp-action="Create">
 <button type="submit" class="btn btn-success">Add to Wallet</button>
</form>