@model EcommerceMVC.Models.DTO.Cart.CartsResponseModel

<table class="table">
    <thead>
         <th>
                RefNo
            </th>
            <th>
                Id
            </th>

            <th>
                DatePaid
            </th>

            <th>
               Totalprice 
            </th>

            <th>
               NumberOfTransaction
            </th>
             <th>
               Actions
            </th>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.Data)
        {
            <tr>
                 <td scope="row">
                    @item.RefNo
                </td>

                <td scope="row">
                    @item.Id
                </td>
                <td scope="row">
                    @item.DatePaid
                </td>
                <td scope="row">
                   @item.TotalPrice
                </td>
                <td scope="row">
                    @item.NumberOfTransaction
                </td>
                <td scope="row">
                   <a class="btn btn-primary">View</a>
                </td>
                
            </tr>
        }
    </tbody>
</table>
<form asp-action="Create">
</form>